  <display-name>13-ajax</display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ajaxdemoServlet</servlet-name>
    <servlet-class>servlet.ajaxdemoServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>judgeServlet</servlet-name>
    <servlet-class>servlet.judgeServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>searchkwServlet</servlet-name>
    <servlet-class>servlet.searchkwServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>selectProServlet</servlet-name>
    <servlet-class>servlet.selectProServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>selectCityServlet</servlet-name>
    <servlet-class>servlet.selectCityServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>ajaxdemoServlet</servlet-name>
    <url-pattern>/ajaxdemoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>judgeServlet</servlet-name>
    <url-pattern>/judgeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>searchkwServlet</servlet-name>
    <url-pattern>/searchkwServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>selectProServlet</servlet-name>
    <url-pattern>/selectProServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>selectCityServlet</servlet-name>
    <url-pattern>/selectCityServlet</url-pattern>
  </servlet-mapping>
 

 
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>